{
  "extends": [
    "google"
  ],
  "env": {
    "jquery": true,
    "browser": true
  },
  "plugins": [
    "lodash"
  ],
  "globals": {
    "_": true,
    "Twine": true
  },
  "rules": {
    "max-len": [
      2,
      {
        "code": 120,
        "tabWidth": 2,
        "ignoreUrls": true,
        "ignoreTrailingComments": true
      }
    ]
  }
}
