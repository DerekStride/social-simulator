<p id="notice"><%= notice %></p>

<h1>Listing Social Users</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @social_users.each do |social_user| %>
      <tr>
        <td><%= social_user.name %></td>
        <td><%= link_to 'Show', social_user %></td>
        <td><%= link_to 'Edit', edit_social_user_path(social_user) %></td>
        <td><%= link_to 'Destroy', social_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Social user', new_social_user_path %>
